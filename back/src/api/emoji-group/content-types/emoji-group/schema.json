{
  "kind": "collectionType",
  "collectionName": "emoji_groups",
  "info": {
    "singularName": "emoji-group",
    "pluralName": "emoji-groups",
    "displayName": "Emoji Groups",
    "description": "Groups of emojis that can be used together and linked in an event."
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "emoji": {
      "type": "component",
      "component": "emoji.emoji",
      "repeatable": true
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "emoji_groups"
    }
  }
}
